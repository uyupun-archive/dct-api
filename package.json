    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli-esm": "^5.0.6"
  },
  "engines": {
    "node": "18.17.1"
  }
}
